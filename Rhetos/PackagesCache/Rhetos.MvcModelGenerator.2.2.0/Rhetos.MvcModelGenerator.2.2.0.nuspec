    <copyright>Copyright Omega software 2015</copyright>
    <tags>Rhetos plugin</tags>
    <dependencies>
      <dependency id="Rhetos" version="1.2.0" />
      <dependency id="Rhetos.CommonConcepts" version="1.2.0" />
      <dependency id="Rhetos.MvcModelGenerator.Client" version="2.2.0" />
    </dependencies>
    <references>
      <group targetFramework=".NETFramework4.5.1">
        <reference file="Rhetos.MvcModelGenerator.DefaultConcepts.dll" />
        <reference file="Rhetos.MvcModelGenerator.dll" />
      </group>
    </references>
  </metadata>
</package>